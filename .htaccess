RewriteEngine On    # Turn on the rewriting engine
RewriteRule    ^about-us/?$    static_pages/about.php    [NC,L] 
RewriteRule    ^contact-us/?$    static_pages/contact.php    [NC,L] 
RewriteRule    ^admin/([a-zA-Z_]+)$   admin/index.php?$1    [L] 
RewriteRule    ^user/setup_profile$   user/profile-setup.php    [L] 
RewriteRule    ^product-category/([a-zA-Z0-9_-]+)$   product-category/index.php?category=$1   [L] 
RewriteRule     ^products/([a-zA-Z0-9_-]+)$    products/index.php?product_code=$1   [L]
